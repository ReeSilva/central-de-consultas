{
"parser": "babel-eslint",
"parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false
    },
    "env": {
        "node": true,
        "mocha": true
        },
        "extends": "airbnb-base",
        "plugins": [
        "babel"
        ],
        "rules": {
            "brace-style": [
            1,
            "stroustrup",
            {
            "allowSingleLine": true
        }
        ],
        "quote-props": 0,
        "strict": 0,
        "global-require": 0,
        "no-continue": 0,
        "no-param-reassign":0,
        "consistent-return": 0,
        "no-unused-vars": 1,
        "max-len": 0,
        "comma-dangle": 0,
        "no-underscore-dangle": 0,
        "generator-star-spacing": 0,
        "camelcase": 0,
        "space-before-function-paren": [1, "always"],
        "indent": [1, 2, { "SwitchCase": 1, "VariableDeclarator": 0}],
        "import/no-dynamic-require": 0,
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true }]
    }
}
